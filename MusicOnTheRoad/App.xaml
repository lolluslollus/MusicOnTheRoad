    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MusicOnTheRoad"
	xmlns:conv="using:MusicOnTheRoad.Converters"
    RequestedTheme="Dark">
	<Application.Resources>
		<!-- Application-specific resources -->
		<ResourceDictionary>
			<conv:BoolToSymbol x:Key="BoolToSymbol"/>
			<conv:BoolToVisible x:Key="BoolToVisible"/>
		</ResourceDictionary>
	</Application.Resources>
</Application>
